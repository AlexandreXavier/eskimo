<?xml version="1.0" encoding="utf-8"?>
<!---
The iOS skin class for the Eskimo UniqueChoiceList component.
@see com.pialabs.eskimo.UniqueChoiceList
-->
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009"
        xmlns:s="library://ns.adobe.com/flex/spark"
        xmlns:mx="library://ns.adobe.com/flex/mx"
        xmlns:components="com.pialabs.eskimo.components.*">
  <!-- host component -->
  <fx:Metadata>
    [HostComponent("com.pialabs.eskimo.components.UniqueChoiceList")]
  </fx:Metadata>

  <!-- SkinParts
  name=listDisplay, type=spark.components.List, required=true
  -->

  <components:ListWheel id="listDisplay" verticalCenter="0" horizontalCenter="0"/>

</s:Skin>
